{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "module": "es6",
    "target": "es2016",
    "allowJs": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@gen/*": [
        "generated/*"
      ]
    }
  }
}
